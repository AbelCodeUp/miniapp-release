{
  "name": "mp_release_platform",
  "version": "1.0.0",
  "private": true,
  "description": "小程序发布平台，详情请看README.md",
  "main": "./private/server/server.js",
  "author": "lzj",
  "publishConfig": {
    "registry": "https://registry.npm.taobao.org"
  },
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon --exec 'ts-node ./server/index.js'",
    "start:pro": "cross-env NODE_ENV=production nodemon --exec 'ts-node ./server/index.js'",
    "start:debug": "cross-env NODE_ENV=production nodemon --exec node -r ts-node/register --inspect ./server/index.js",
    "start:only": "npm run start:only:dev",
    "start:only:dev": "cross-env NODE_ENV=development NOT_HOT='true' nodemon --exec 'ts-node ./server/index.js'",
    "start:only:pro": "cross-env NODE_ENV=production NOT_HOT='true' nodemon --exec 'ts-node ./server/index.js'",
    "build:pro": "rm -rf ./public/bundle && npm run build:client:pro && npm run build:server:pro && npm run build:nest:server:pro && echo '生产环境m站编译完成'",
    "build:client:pro": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.config.js --progress --colors --display-error-details && echo '生产环境m站客户端渲染部分编译完成'",
    "build:server:pro": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.config.js --progress --colors --display-error-details && echo '生产环境m站服务端渲染部分编译完成'",
    "build:nest:server:pro": "rm -rf ./private/server && cross-env NODE_ENV='production' nest build && echo '生产环境服务器编译完成'",
    "build:dev": "rm -rf ./public/bundle && npm run build:client:dev && npm run build:server:dev && echo '开发环境m站编译完成'",
    "build:client:dev": "cross-env NODE_ENV=development webpack --config ./build/webpack.client.config.js --progress --colors --display-error-details && echo '开发环境m站客户端渲染部分编译完成'",
    "build:server:dev": "cross-env NODE_ENV=development webpack --config ./build/webpack.server.config.js --progress --colors --display-error-details && echo '开发环境m站服务端渲染部分编译完成'",
    "lint": "npm run lint:client && lint:server && echo '所有代码符合规范!'",
    "lint:server": "eslint './server/**/*.ts' && echo '服务端代码符合规范!'",
    "lint:client": "eslint app && echo '客户端代码符合规范!'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": "eslint --ext .js",
    "**/*.ts": "eslint --ext .ts",
    "**/*.vue": "eslint --ext .vue"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^7.4.4",
    "@nestjs/websockets": "^7.4.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/socket.io": "^2.1.11",
    "alipay-dev": "^0.5.3",
    "autoprefixer": "^9.8.6",
    "axios": "^0.19.2",
    "babel-plugin-component": "^1.1.1",
    "body-parser": "^1.19.0",
    "chokidar": "^3.4.2",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "cssnano": "^4.1.10",
    "dayjs": "^1.7.7",
    "download-git-repo": "^3.0.2",
    "element-ui": "^2.14.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "html-webpack-plugin": "^4.3.0",
    "lodash": "^4.17.11",
    "memory-fs": "^0.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "miniprogram-ci": "^1.0.83",
    "mockjs": "^1.1.0",
    "mysql2": "^2.2.5",
    "node-rsa": "^1.1.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "qs": "^6.9.4",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sequelize": "^6.3.5",
    "sequelize-typescript": "^2.0.0-beta.0",
    "shelljs": "^0.8.4",
    "socket.io-client": "^2.3.1",
    "tt-ide-cli": "0.0.3",
    "url": "^0.11.0",
    "uuid": "^8.3.1",
    "vconsole": "^3.3.4",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.3",
    "vue-router": "^3.4.3",
    "vue-server-renderer": "^2.6.11",
    "vuelidate": "^0.7.4",
    "vuex": "^3.5.1",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^4.44.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.1.1",
    "webpack-node-externals": "^2.5.0",
    "wechat-api": "^1.35.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.0",
    "eslint": "7.1.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "husky": "^4.2.5",
    "jest": "26.0.1",
    "lint-staged": "^10.4.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "sass-loader": "^9.0.2",
    "sass-resources-loader": "^2.0.3",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "url-loader": "^4.1.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lizijie123/mp_release_platform.git"
  },
  "keywords": [
    "vue",
    "nest",
    "typescript",
    "mp",
    "ci",
    "mp-co"
  ],
  "license": "ISC"
}
